# Root-level defaults shared across agents.
# Copy this file to .env and fill in values. Agent-specific overrides live in agents/<agent>/.env.

LOG_LEVEL=info

# LLM providers (optional; set the ones you need)
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Runtime / observability
RUNTIME_BASE_URL=http://localhost:3000
TRACE_EXPORTER_ENDPOINT=http://localhost:4318
